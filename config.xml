<?xml version='1.0' encoding='utf-8'?>
<widget id="net.jsguys.beerific" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Beerific</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="info@jsguys.net" href="http://www.jsguys.net">
        jsguys
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/android-icon-36x36.png" density="ldpi" />
        <icon src="res/android/android-icon-48x48.png" density="mdpi" />
        <icon src="res/android/android-icon-72x72.png" density="hdpi" />
        <icon src="res/android/android-icon-96x96.png" density="xhdpi" />
        <icon src="res/android/android-icon-144x144.png" density="xxhdpi" />
        <icon src="res/android/android-icon-192x192.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
